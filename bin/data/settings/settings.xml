<rhizopus>
  <canvas>
    <framerate>20</framerate>
    <antialiasing>0</antialiasing>
    <osdActive>1</osdActive>
    <osdColor> <r>255</r> <g>255</g> <b>255</b> <a>255</a> </osdColor>
    <backgroundColor> <r>0</r> <g>0</g> <b>0</b> <a>255</a> </backgroundColor>
  </canvas>
  <field>
    <zeroThreshold><min>0.05</min><max>0.05</max></zeroThreshold>
  </field>
  <hypha>
    <color> <r>0</r> <g>0</g> <b>127</b> <a>2</a> </color>
    <speed><min>0.9</min><max>0.9</max></speed> <!--pixels/s-->
    <maxGrowthPercentage><min>0.2</min><max>0.2</max></maxGrowthPercentage>
    <birthAreaRadius><min>3</min><max>3</max></birthAreaRadius>
    <birthRays><min>1</min><max>1</max></birthRays>
    <birthRayDirections><min>1</min><max>1</max></birthRayDirections>
    <energySpentToMove>
      <min> <min>0.002</min><max>0.002</max> </min>
      <max> <min>0.02</min><max>0.02</max> </max>
    </energySpentToMove>
    <foodToEnergyRatio><min>0.015</min> <max>0.015</max></foodToEnergyRatio>
    <maxBendAngleDeg><min>4</min> <max>4</max></maxBendAngleDeg>
    <forkDistance>
      <min><min>5</min><max>5</max></min>
      <max><min>20</min><max>20</max></max>
    </forkDistance>
    <maxForkAngleDeg><min>30</min><max>30</max></maxBendAngleDeg>
    <pixelOverlap>1</pixelOverlap>
  </hypha>
</rhizopus>
