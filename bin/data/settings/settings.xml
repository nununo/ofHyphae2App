<rhizopus>
  <canvas>
    <framerate>10</framerate>
    <antialiasing>0</antialiasing>
    <osdActive>0</osdActive>
    <osdColor>
      <r>255</r>
      <g>255</g>
      <b>255</b>
      <a>255</a>
    </osdColor>
    <backgroundColor>
      <r>0</r>
      <g>0</g>
      <b>0</b>
      <a>255</a>
    </backgroundColor>
  </canvas>
  <field>
    <zeroThreshold>
      <min>0.5</min>
      <max>0.5</max>
    </zeroThreshold>
  </field>
  <hypha>
    <color>
      <r>255</r>
      <g>0</g>
      <b>0</b>
      <a>10</a>
    </color>
    <speed>
      <min>0.9</min>
      <max>0.9</max>
    </speed> <!--pixels/s-->
    <energySpentToMove>
      <min>0.01</min>
      <max>0.01</max>
    </energySpentToMove>
    <maxBendAngleDeg>
      <min>1</min>
      <max>5</max>
    </maxBendAngleDeg>
    <foodAmount>
      <min>0.1</min>
      <max>0.1</max>
    </foodAmount>
    <eatSpendRatio>
      <min>2</min>
      <max>2</max>
    </eatSpendRatio>
    <forkDistanceMin>
      <min>15</min>
      <max>15</max>
    </forkDistanceMin>
    <forkDistanceMax>
      <min>50</min>
      <max>50</max>
    </forkDistanceMax>
    <maxForkAngleDeg>
      <min>1</min>
      <max>30</max>
    </maxBendAngleDeg>
    <pixelOverlap>1</pixelOverlap>
  </hypha>
</rhizopus>
