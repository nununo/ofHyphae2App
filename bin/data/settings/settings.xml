<rhizopus>
  <canvas>
    <framerate>30</framerate>
    <antialiasing>0</antialiasing>
    <osdActive>1</osdActive>
    <osdColor> <r>0</r> <g>0</g> <b>0</b> <a>255</a> </osdColor>
    <backgroundColor> <r>255</r> <g>255</g> <b>255</b> <a>255</a> </backgroundColor>
  </canvas>
  <field>
    <frequency><min>1</min><max>2</max></frequency>
    <amplification><min>0.5</min><max>1</max></amplification>
  </field>
  <hypha>
    <maxElements><min>20000</min><max>40000</max></maxElements>
    <color> <r>255</r> <g>255</g> <b>255</b> <a>4</a> </color>
    <speed> <!--pixels/s-->
      <min> <min>0.9</min><max>0.9</max> </min>
      <max> <min>1.0</min><max>1.0</max> </max>
    </speed>
    <maxGrowthPercentage><min>0.2</min><max>0.2</max></maxGrowthPercentage>
    <birthRays><min>1</min><max>100</max></birthRays>
    <birthRayDirections><min>1</min><max>10</max></birthRayDirections>
    <energySpentToMove>
      <min> <min>0.002</min><max>0.002</max> </min>
      <max> <min>0.02</min><max>0.02</max> </max>
    </energySpentToMove>
    <foodToEnergyRatio><min>0.015</min> <max>0.015</max></foodToEnergyRatio>
    <maxBendAngleDeg><min>0.5</min> <max>3</max></maxBendAngleDeg>
    <maxForkAngleDeg><min>5</min><max>30</max></maxForkAngleDeg>
    <maxAngleWithInitialDirection><min>90</min><max>100</max></maxAngleWithInitialDirection>
    <forkDistance>
      <min><min>5</min><max>5</max></min>
      <max><min>20</min><max>20</max></max>
    </forkDistance>
    <fertilityRatio><min>0.5</min><max>0.5</max></fertilityRatio>
  </hypha>
</rhizopus>
